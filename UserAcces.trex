<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.tableau.extensions.auto-params" extension-version="1.0.0">
    <default-locale>es_ES</default-locale>
    <name resource-id="name"/>
    <description>Extensión para alimentar parámetros automáticamente al abrir el dashboard</description>
    <author name="Your Company" email="info@yourcompany.com" organization="Your Company" website="https://www.yourcompany.com"/>
    <min-api-version>1.4</min-api-version>
    <source-location>
      <url>https://canidoac.github.io/UserAcces/index.html</url>
    </source-location>
    <icon>
    
    </icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
    <!-- Eliminado <context-menu> porque usamos botón de configuración -->
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="es_ES">Auto Parámetros</text>
    </resource>
  </resources>
</manifest>
